{
  "compilerOptions": {
    "target": "ES2022",
    "module": "esnext",
    "moduleResolution": "bundler",
    "lib": [
      "es2022",
      "dom"
    ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "declaration": true,
    "sourceMap": true,
    "composite": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    //"ignoreDeprecations": "6.0"
  }
}