{
  "name": "jsp-monorepo",
  "private": true,
  "version": "1.0.0",
  "description": "Monorepo para o ecossistema JSP (Web e API)",
  "author": "JSP Servicos",
  "scripts": {
    "dev:web": "pnpm --filter @jsp/web dev",
    "dev:api": "pnpm --filter @jsp/api start:dev",
    "build:web": "pnpm --filter @jsp/web build",
    "build:api": "pnpm --filter @jsp/api build",
    "lint": "pnpm --filter \"@jsp/..\" lint",
    "db:dev:up": "docker-compose up -d postgres",
    "db:dev:generate": "pnpm --filter @jsp/api exec prisma generate",
    "db:dev:migrate": "pnpm --filter @jsp/api exec prisma migrate dev",
    "db:dev:studio": "pnpm --filter @jsp/api exec prisma studio"
  },
  "devDependencies": {
    "@types/node": "^20.19.24",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@9.4.0"
}