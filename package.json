{
  "name": "intermedius-backend",
  "version": "1.0.0",
  "description": "Intermedius Coach - Real-time negotiation (WhatsApp + Voice)",
  "private": true,
  "scripts": {
    "build": "nest build",
    "build:all": "nest build api-gateway && nest build ingestor && nest build policy-service",
    "start": "node dist/apps/api-gateway/main",
    "start:dev": "nest start --watch",
    "start:dev:gateway": "nest start --watch api-gateway",
    "start:dev:ingestor": "nest start --watch ingestor",
    "start:dev:policy": "nest start --watch policy-service",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{apps,libs}/**/*.ts\" --fix",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "seed": "ts-node libs/prisma/seeds/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api-gateway/test/jest-e2e.json"
  },
  "prisma": {
    "schema": "libs/prisma/schema.prisma"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ranmdom/Intermedius-BackEnd.git"
  },
  "keywords": [
    "nestjs",
    "prisma",
    "fastify",
    "real-time"
  ],
  "author": "BX Consultoria",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ranmdom/Intermedius-BackEnd/issues"
  },
  "homepage": "https://github.com/Ranmdom/Intermedius-BackEnd#readme",
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "prisma": "^6.16.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@fastify/static": "^8.2.0",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.64.6",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@prisma/client": "^6.16.3",
    "argon2": "^0.44.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "fastify": "^4.28.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "rxjs": "^7.8.2"
  }
}
